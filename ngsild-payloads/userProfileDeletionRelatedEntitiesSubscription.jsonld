{
    "id": "urn:ngsi-ld:Subscription:UserProfileDeletion:RelatedEntitiesNotification",
    "type": "Subscription",
    "entities": [
        {
            "type": "UserProfile"
        }
    ],
    "notificationTrigger": [
        "entityDeleted"
    ],
    "notification": {
        "format": "keyValues",
        "endpoint": {
            "uri": "http://localhost:8080/userDeletionListener",
            "accept": "application/json"
        },
        "attributes": [
            "hasConsent",
            "hasFluidMonitoring"
        ]
    }
}